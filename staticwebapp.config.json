{
  "routes": [
    {
      "route": "/login",
      "redirect": "/.auth/login/github",
      "statusCode": 301
    },
    {
      "route": "/",
      "rewrite": "/.auth/login/github?post_login_redirect_uri=.referrer",
      "statusCode": 302
    },
    {
      "route": "/.auth/login/github",
      "statusCode": 200
    },
    {
      "route": "/.auth/logout",
      "statusCode": 200
    }
  ]
}