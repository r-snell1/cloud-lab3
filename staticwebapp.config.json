{
  "routes": [
    {
      "route": "/login",
      "redirect": "/.auth/login/github"
    },
    {
      "route": "/",
      "rewrite": "/.auth/login/github?post_login_redirect_uri=/welcome"
    },
    {
      "route": "/welcome",
      "statusCode": 200,
      "response": {
        "contentType": "text/html",
        "content": "<h1>My name is Ryan!</h1>"
      }
    }
  ]
}
